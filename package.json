  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist",
    "test": "jest",
    "prepare": "npm run clean && npm run build"
  },
  "author": "yayoc <hi@yayoc.com> (http://yayoc.com)",
  "license": "MIT",
  "devDependencies": {
    "@types/eslint": "^6.1.3",
